<h1 id="log_in">Doctor Log In</h1>
<%= form_tag(@doctor) do %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag(:password) %>
  </div>
  <div class="action">
    <%= submit_tag "Log in" %>
  </div>
<% end %>
